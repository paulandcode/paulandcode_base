<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.paulandcode.dao.SysUserDao">
	
	<select id="getUserByName" parameterType="map" resultType="com.paulandcode.entity.SysUserEntity">
		SELECT
			id,
			name
		FROM 
			user
		WHERE
			name = #{name}
	</select>		

</mapper>




























